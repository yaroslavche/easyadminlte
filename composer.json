{
    "name": "yaroslavche/easyadminlte",
    "type": "symfony-bundle",
    "description": "EasyAdmin AdminLTE layout",
    "keywords": ["symfony", "symfony-bundle", "easyadmin", "easyadmin-extension "],
    "license": "MIT",
    "authors": [
        {
            "name": "yaroslavche",
            "email": "yaroslav429@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yaroslavche\\EasyAdminLTE\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yaroslavche\\EasyAdminLTE\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/http-kernel": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "symfony/framework-bundle": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/var-dumper": "^6.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
